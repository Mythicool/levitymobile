# Development Environment Variables for Levity Loyalty Mobile App

# Supabase Configuration (Development Database)
# EXPO_PUBLIC_SUPABASE_URL=https://your-dev-project-id.supabase.co
# EXPO_PUBLIC_SUPABASE_ANON_KEY=your-dev-anon-key-here
EXPO_PUBLIC_USE_PRODUCTION_DB=false

# API Configuration (Development)
EXPO_PUBLIC_API_URL=http://localhost:3000
EXPO_PUBLIC_API_VERSION=v1

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_DEBUG=true

# Analytics (disabled in development)
EXPO_PUBLIC_ANALYTICS_ENABLED=false

# Feature Flags
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_HAPTICS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true

# Demo Configuration (always enabled in development)
EXPO_PUBLIC_DEMO_MODE=true
EXPO_PUBLIC_DEMO_EMAIL=demo@levity.com
EXPO_PUBLIC_DEMO_PASSWORD=demo123

# Database Configuration
EXPO_PUBLIC_DB_PROVIDER=mock
