# Production Environment Variables for Levity Loyalty Mobile App

# API Configuration
EXPO_PUBLIC_API_URL=https://levity-loyalty-mobile.netlify.app
EXPO_PUBLIC_API_VERSION=v1

# App Configuration
EXPO_PUBLIC_APP_ENV=production
EXPO_PUBLIC_DEBUG=false

# Analytics (if needed)
EXPO_PUBLIC_ANALYTICS_ENABLED=true

# Feature Flags
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_HAPTICS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true

# Demo Configuration
EXPO_PUBLIC_DEMO_MODE=true
EXPO_PUBLIC_DEMO_EMAIL=demo@levity.com
EXPO_PUBLIC_DEMO_PASSWORD=demo123
